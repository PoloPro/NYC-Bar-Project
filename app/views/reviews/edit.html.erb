<% if flash[:alert] %>
    <div class="alert"><%= flash[:alert] %></div>
<% end %>
<h3> Edit Your Review of <%=@review.bar.name%></h3>
<%= form_for @review do |f| %>
  <%= f.label :rating, "Rating" %><br>
  <%= f.text_field :rating %><br>
  <%= f.label :content, "Review" %><br>
  <%= f.text_area :content%><br>  
  <%= f.submit %>
<% end %>